<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ExiaplayagainBundle\Entity\Games" table="games">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false"/>
    <field name="type" type="string" column="type" length="255" nullable="false"/>
    <field name="availability" type="string" column="availability" length="255" nullable="false"/>
    <field name="price" type="integer" column="price" nullable="true"/>
    <field name="info" type="text" column="info" length="65535" nullable="true"/>
    <field name="url" type="string" column="url" length="255" nullable="true"/>
    <field name="image" type="string" column="image" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
